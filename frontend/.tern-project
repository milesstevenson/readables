{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "react"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "node_modules/semantic-ui-react/*.js",
    "node_modules/redux/dist/*.js",
    "node_modules/react/*.js",
    "node_modules/react-redux/dist/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "lint": {},
    "requirejs": {},
    "modules": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}